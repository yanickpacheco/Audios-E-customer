		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            DataTable tabla = new DataTable();
            OleDbDataAdapter adapter = new OleDbDataAdapter();
            adapter.Fill(tabla, Dts.Variables["ListaAudios"].Value);

            foreach (DataRow row in tabla.Rows)
            {
                descargaGrabacion(row["fechaVenta"].ToString()
                    , row["idLlamada"].ToString()
                    , row["crm"].ToString()
                    , row["formato"].ToString()
                    , row["idCliente"].ToString()
                    , row["rutaAudiosLocal"].ToString());
            }

            tabla = new DataTable();
            adapter = new OleDbDataAdapter();
            adapter.Fill(tabla, Dts.Variables["Rutas"].Value);
            DataTable tablaAux = new DataTable();
            tablaAux = tabla.Clone();

            foreach (DataRow row in tabla.Rows)
            {
                if (Directory.Exists(row["rutaAudiosLocal"].ToString()))
                {
                    tablaAux.ImportRow(row);
                }
            }

            Dts.Variables["Rutas"].Value = tablaAux;

            Dts.TaskResult = (int)ScriptResults.Success;
		}

        private void descargaGrabacion(string _fechaVenta, string _idLlamada, string _crm, string _formato,string _idCliente,string _rutaAudiosLocal)
        {
            string dia;
            string Mes;
            string anio;
            string rutaOrigen;
            string rutaDestino;

            dia = _fechaVenta.Substring(6, 2);
            Mes = _fechaVenta.Substring(4, 2);
            anio = _fechaVenta.Substring(0, 4);

            string nomFinalcrm = Int64.Parse(_crm).ToString("D3");

            // 'Se obtienen las rutas en donde estén depositadas las grabaciones
            DataTable tablaGrabaciones = new DataTable();
            clsGrabaciones grabaciones = new clsGrabaciones();
            tablaGrabaciones = grabaciones.obtieneRutasGrabaciones();
            
            rutaDestino = _rutaAudiosLocal;
            foreach (DataRow row in tablaGrabaciones.Rows)
            {
                try
                {
                    rutaOrigen = row["rutaGrabaciones"].ToString();
                    rutaOrigen = rutaOrigen.Replace("[crm]", nomFinalcrm.ToString());
                    rutaOrigen = rutaOrigen.Replace("[anio]", anio);
                    rutaOrigen = rutaOrigen.Replace("[mes]", Mes);
                    rutaOrigen = rutaOrigen.Replace("[dia]", dia);

                    System.IO.DirectoryInfo di = new System.IO.DirectoryInfo(rutaOrigen);
                    var idLlamadaAux = Int64.Parse(_idLlamada);
                    System.IO.FileInfo[] aryFi = di.GetFiles("*" + idLlamadaAux.ToString() + "*");
                    rutaOrigen = rutaOrigen + @"\" + aryFi[0].ToString();

                    if (File.Exists(rutaOrigen)) {
                        Directory.CreateDirectory(rutaDestino);
                        if (!File.Exists(rutaDestino + _formato))
                        {
                            File.Copy(rutaOrigen, rutaDestino + _formato);
                            MacarRegistros(_idCliente, _crm);
                        }
                        break;
                    }
                }
                catch (Exception ex)
                {
                }
            }
        }

        private void MacarRegistros(string _idCliente,string _crm)
        {
            using (SqlConnection cnn = new SqlConnection())
            {
                cnn.ConnectionString = "data source = 192.168.1.14; initial catalog = VENTAS; User id= cp; password=Cordial.passs ";
                var Sql = new SqlCommand("Proceso.pa_MarcaAudioEncontrado", cnn);
                Sql.CommandType = CommandType.StoredProcedure;
                Sql.Parameters.AddWithValue("@crm", _crm);
                Sql.Parameters.AddWithValue("@idCliente", _idCliente);
                
                cnn.Open();
                if (cnn.State == ConnectionState.Open)
                {
                    Sql.ExecuteNonQuery();
                    cnn.Close();
                }
            }
        }
        
